{
  "name": "bun-hono-nest-like",
  "version": "1.0.0",
  "scripts": {
    "dev": "bun run --hot src/main.ts --target bun",
    "build": "bun build src/main.ts --outdir dist --target bun",
    "start": "bun run src/main.js --target bun",
    "prepare": "husky install",
    "migration:create": "mikro-orm migration:create",
    "migration:up": "mikro-orm migration:up",
    "migration:down": "mikro-orm migration:down",
    "migrations:fresh": "mikro-orm migration:fresh",
    "lint:check": "eslint src --ext .ts,.js --max-warnings=0 --ignore-path .eslintignore",
    "format:check": "prettier --check src --ignore-path .prettierignore",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@mikro-orm/cli": "6.5.6",
    "@mikro-orm/migrations": "6.5.6",
    "@mikro-orm/postgresql": "6.5.6",
    "ajv": "8.17.1",
    "convict": "^6.2.4",
    "hono": "4.9.8",
    "inversify": "7.10.1",
    "pino": "9.11.0",
    "pino-pretty": "13.1.1",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@types/bun": "1.2.22",
    "@types/convict": "^6.1.6",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "8.42.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.0",
    "husky": "^9.1.7",
    "prettier": "3.0.0",
    "source-map-support": "0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "5.1.3",
    "vitest": "^3.2.4"
  }
}
